{
  "name": "javascript-asteroids",
  "version": "0.0.1",
  "description": "A single page application of the game of Asteroids.",
  "main": "app.js",
  "repository": "git@github.com:hughes-ch/asteroids.git",
  "author": "hughes-ch <contact@chrishughesdev.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "babel": "^6.23.0",
    "jest": "^27.2.4",
    "webpack-cli": "^4.9.0"
  },
  "scripts": {
    "api-test": "yarn build && pytest -v --cov=api/src --cov-report=term-missing",
    "api-install": "pip install -r api/requirements.txt",
    "build": "webpack",
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test": "jest --coverage",
    "start-flask": "yarn build && export FLASK_APP=api/src && export FLASK_ENV='development' && flask run --host 0.0.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "enumify": "^2.0.0",
    "mathjs": "^9.5.0",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.58.1"
  }
}
